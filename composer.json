{
    "name": "homenet.dev/routeros-api",
    "description": "This is my package routeros-api",
    "keywords": [
        "HomeNet",
        "routeros-api"
    ],
    "homepage": "https://github.com/homenet.dev/routeros-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Rupadana",
            "email": "rupadanawayan@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^2.20",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "HomeNet\\RouterosApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HomeNet\\RouterosApi\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
